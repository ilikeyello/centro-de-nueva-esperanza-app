\v 6 \w Y|strong="H3068"\w* \w los|strong="H1931"\w* \w cielos|strong="H8064"\w* declaran \w su|strong="H1931"\w* \w justicia|strong="H6664"\w*; \w porque|strong="H3588"\w* Dios \w mismo|strong="H1931"\w* \w es|strong="H1931"\w* \w el|strong="H1931"\w* \w juez|strong="H8199"\w*. \w Selah|strong="H5542"\w*.   
\p
\v 7 \w Escucha|strong="H8085"\w*, \w pueblo|strong="H5971"\w* \w mío|strong="H5971"\w*, \w a|strong="H3068"\w* mis \w palabras|strong="H1696"\w*; Oh \w Israel|strong="H3478"\w*, seré \w testigo|strong="H5749"\w* \w contra|strong="H3478"\w* ti; Yo soy Dios, tu Dios.   
\p
\v 8 \w No|strong="H3808"\w* tomaré una causa \w contra|strong="H5921"\w* \w ti|strong="H5921"\w* \w por|strong="H5921"\w* tus ofrendas, \w ni|strong="H3808"\w* \w por|strong="H5921"\w* tus ofrendas quemadas, \w que|strong="H3808"\w* están \w siempre|strong="H8548"\w* ante \w mí|strong="H5921"\w*.   
